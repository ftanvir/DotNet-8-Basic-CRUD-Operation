@{
}
@model List<StudentPortal.Web.Models.Entities.Student>
<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th></th>
        </tr>

        <tbody>
            @foreach(var students in Model)             {
                <tr>
                    <td>@students.Id</td>
                    <td>@students.Name</td>
                    <td>@students.Email</td>
                    <td>
                        <a>Edit</a>
                    </td>
                </tr>
            }
        </tbody>
    </thead>
</table>
